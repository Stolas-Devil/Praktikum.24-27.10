function phi(table) {
  return (table[3] * table[0] - table[2] * table[1]) /
    Math.sqrt((table[2] + table[3]) *
              (table[0] + table[1]) *
              (table[1] + table[3]) *
              (table[0] + table[2]));
} // flashback to 10th grade math class

console.log(phi([76, 9, 4, 1])); // fil the string before use 